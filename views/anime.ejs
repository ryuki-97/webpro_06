<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>アニメ管理</title>
</head>
<body>
  <h2>アニメ管理</h2>
  <table>
    <tr><th>ID</th><th>面白さ</th><th>作品名</th></tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr><td><%= row.id %></td>
        <td><%= row.funny %></td>
        <td><a href="/anime/<%= number %>"><%= row.title %></a></td></tr>
        <% number += 1 %>
      <% } %>
  </table>
  <a href="/anime/create">追加</a>
</body>
</html>